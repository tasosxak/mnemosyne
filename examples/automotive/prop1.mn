on instance(int vDetected, int vGroundTruth, int steering, int speed) do
    output bool correctDetection, int deltaSteering, int deltaSpeed;
    correctDetection <- vDetected = vGroundTruth;
    deltaSteering <-  |steering - @steering[steering]|;
    deltaSpeed <- speed - @speed[speed];
    send instance(correctDetection, deltaSteering, deltaSpeed);
end