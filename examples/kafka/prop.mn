on open(string file) do
    output string id;
    id <- file;
    send open(id);
end

on close(string file) do
    output string id;
    id <- file;
    send close(id);
end

on write(string file, string user) do
    output string fileId, string userId;
    fileId <- file;
    userId <- user;
    send userId(fileId, userId);
end