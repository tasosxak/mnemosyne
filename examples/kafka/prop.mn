on open(int file) do
    output int id;
    id <- file;
    send open(id);
end

on close(int file) do
    output int id;
    id <- file;
    send close(id);
end

on write(int file, int user) do
    output int fileId, int userId;
    fileId <- file;
    userId <- user;
    send userId(fileId, userId);
end