on set(string ac, int temp) do
    output bool InBound, string Ac, int Temp;
    InBound <- temp >= 17 and temp <= 26;
    Ac <- ac;
    Temp <- temp;
    send set(Ac,Temp,InBound);
end

on turn_on(string ac) do
    output string Ac;
    Ac <- ac;
    send turn_on(Ac);
end

on turn_off(string ac) do
    output string Ac;
    Ac <- ac;
    send turn_off(Ac);
end